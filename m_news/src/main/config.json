{
  "app": {
    "bundleName": "com.example.harmonyapp",
    "vendor": "example",
    "version": {
      "code": 1,
      "name": "1.0"
    },
    "apiVersion": {
      "compatible": 3,
      "target": 4,
      "releaseType": "Beta1"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.example.m_news",
    "name": ".MyApplication",
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "m_news",
      "moduleType": "feature"
    },
    "abilities": [
      {
        "orientation": "portrait",
        "visible": true,
        "name": "com.example.m_news.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "m_news",
        "type": "page",
        "launchType": "standard"
      },
      {
        "name": "com.example.m_news.SharedService",
        "type": "service"
      }
    ],
    "reqPermissions": [
      {
        "name": "ohos.permission.servicebus.ACCESS_SERVICE"
      },
      {
        "name": "ohos.permission.DISTRIBUTED_DATASYNC"
      },
      {
        "name": "ohos.permission.DISTRIBUTED_DEVICE_STATE_CHANGE"
      },
      {
        "name": "ohos.permission.GET_DISTRIBUTED_DEVICE_INFO"
      },
      {
        "name": "ohos.permission.SYSTEM_FLOAT_WINDOW"
      },
      {
        "name": "ohos.permission.INTERNET"
      },
      {
        "name": "ohos.permission.R"
      }
    ]
  }
}